<%= simple_form_for(@sub_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :order_id, collection: Order.all.map { |order| [order.name, order.id] }%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
