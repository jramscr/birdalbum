<p id="notice"><%= notice %></p>

<h1>Listing Sub Orders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_orders.each do |sub_order| %>
      <tr>
        <td><%= sub_order.name %></td>
        <td><%= link_to 'Show', sub_order %></td>
        <td><%= link_to 'Edit', edit_sub_order_path(sub_order) %></td>
        <td><%= link_to 'Destroy', sub_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub order', new_sub_order_path %>
