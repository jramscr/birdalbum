<p id="notice"><%= notice %></p>

<h1>Listing Quantity Eggs</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quantity_eggs.each do |quantity_egg| %>
      <tr>
        <td><%= quantity_egg.description %></td>
        <td><%= link_to 'Show', quantity_egg %></td>
        <td><%= link_to 'Edit', edit_quantity_egg_path(quantity_egg) %></td>
        <td><%= link_to 'Destroy', quantity_egg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quantity egg', new_quantity_egg_path %>
